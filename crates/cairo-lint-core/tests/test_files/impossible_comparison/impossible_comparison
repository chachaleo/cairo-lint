//! > ==========================================================================

//! > le and ge

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x <= 100 && x >= 200 {
        //impossible to reach
    }
}

//! > diagnostics
error: Plugin diagnostic: Impossible condition, always false
 --> lib.cairo:3:8
  |
3 |     if x <= 100 && x >= 200 {
  |        ^^^^^^^^^^^^^^^^^^^^
  |

//! > fixed
fn main() {
    let x: u32 = 1;
    if x <= 100 && x >= 200 {
        //impossible to reach
    }
}

//! > ==========================================================================

//! > le and ge allowed

//! > cairo_code
fn main() {
    let x: u32 = 1;
    #[allow(impossible_comparison)]
    if x <= 100 && x >= 200 {
        //impossible to reach
    }
}

//! > diagnostics

//! > fixed
fn main() {
    let x: u32 = 1;
    #[allow(impossible_comparison)]
    if x <= 100 && x >= 200 {
        //impossible to reach
    }
}

//! > ==========================================================================

//! > le and ge different var

//! > cairo_code
fn main() {
    let x: u32 = 1;
    let y: u32 = 1;
    if x <= 100 && y >= 200 {
        //possible to reach
    }
}

//! > diagnostics

//! > fixed
fn main() {
    let x: u32 = 1;
    let y: u32 = 1;
    if x <= 100 && y >= 200 {
        //possible to reach
    }
}

//! > ==========================================================================

//! > with else clause

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x >= 200 && x < 100 {
        //impossible to reach
    } else if x == 1 {
        //possible to reach
    } else {
        //possible to reach
    }
}

//! > diagnostics
error: Plugin diagnostic: Impossible condition, always false
 --> lib.cairo:3:8
  |
3 |     if x >= 200 && x < 100 {
  |        ^^^^^^^^^^^^^^^^^^^
  |

//! > fixed
fn main() {
    let x: u32 = 1;
    if x >= 200 && x < 100 {
        //impossible to reach
    } else if x == 1 {
        //possible to reach
    } else {
        //possible to reach
    }
}

