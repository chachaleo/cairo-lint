//! > gt and lt

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x > 200 && x < 100 {
        //impossible to reach
    }
}

//! > diagnostics
warning: Plugin diagnostic: Impossible condition, replace by false
 --> lib.cairo:3:5
  |
3 |       if x > 200 && x < 100 {
  |  _____-
4 | |         //impossible to reach
5 | |     }
  | |_____-
  |

//! > fixed
fn main() {
    let x: u32 = 1;
    if false {
        //impossible to reach
    }
}

