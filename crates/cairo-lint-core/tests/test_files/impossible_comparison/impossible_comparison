//! > ==========================================================================

//! > le and gt

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x <= 100 && x > 200 {
        //impossible to reach
    }
}

//! > diagnostics
error: Plugin diagnostic: Impossible condition, always false
 --> lib.cairo:3:8
  |
3 |     if x <= 100 && x > 200 {
  |        ^^^^^^^^^^^^^^^^^^^
  |

//! > fixed
fn main() {
    let x: u32 = 1;
    if x <= 100 && x > 200 {
