//! > gt and lt

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x > 200 && x < 100 {
        //impossible to reach
    }
}

//! > diagnostics
error: Plugin diagnostic: Impossible condition, always false
 --> lib.cairo:3:8
  |
3 |     if x > 200 && x < 100 {
  |        ^^^^^^^^^^^^^^^^^^
  |

//! > fixed
fn main() {
    let x: u32 = 1;
    if x > 200 && x < 100 {
        //impossible to reach
    }
}

//! > ==========================================================================

//! > gt and lt possible

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x > 100 && x < 105 {
        //possible to reach
    }
}

//! > diagnostics

//! > fixed
fn main() {
    let x: u32 = 1;
    if x > 100 && x < 105 {
        //possible to reach
    }
}

//! > ==========================================================================

