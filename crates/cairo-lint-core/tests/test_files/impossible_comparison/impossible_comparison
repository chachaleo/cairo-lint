//! > ==========================================================================

//! > le and ge allowed

//! > cairo_code
fn main() {
    let x: u32 = 1;
    #[allow(impossible_comparison)]
    if x <= 100 && x >= 200 {
        //impossible to reach
    }
}

//! > diagnostics

//! > fixed
fn main() {
    let x: u32 = 1;
    #[allow(impossible_comparison)]
    if x <= 100 && x >= 200 {
        //impossible to reach
    }
}

//! > ==========================================================================

//! > with else clause

//! > cairo_code
fn main() {
    let x: u32 = 1;
    if x >= 200 && x < 100 {
        //impossible to reach
    } else if x == 1 {
        //possible to reach
    } else {
        //possible to reach
    }
}

//! > diagnostics
warning: Plugin diagnostic: Impossible condition, replace by false
 --> lib.cairo:3:5
  |
3 |       if x >= 200 && x < 100 {
  |  _____-
4 | |         //impossible to reach
... |
8 | |         //possible to reach
9 | |     }
  | |_____-
  |

//! > fixed
fn main() {
    let x: u32 = 1;
    if false {
        //impossible to reach
    } else if x == 1 {
        //possible to reach
    } else {
        //possible to reach
    }
}

